@{
    Layout = null;
}



@{
    Layout = null;
}

<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2 id="form-theme"><small id="commonSetting">Common Settings</small></h2>
                <div class="clearfix"></div>
            </div>
            <div class="x_content" style="display:block">
                <br>
                <form id="add-setting-form" data-parsley-validate="" class="form-horizontal form-label-left" novalidate="">
                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="value">
                            Value <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <textarea id="value"
                                      class="form-control"
                                      name="value"></textarea>
                        </div>
                    </div>

                    <div class="ln_solid"></div>

                    <div class="form-group">
                        <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                            <button id="cancel-btn" disabled="disabled" class="btn btn-primary" type="button">Cancel</button>
                            <button id="save-btn" disabled="disabled" class="btn btn-success" type="submit">Save</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>